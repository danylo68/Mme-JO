width=\"width\"\n        height=\"height\">\n  \n  <div class=\"button_graphe\">\n    <v-dialog\n      v-model=\"dialog\"\n      width=\"900\"\n      height=\"700\"\n    >\n      <template v-slot:activator=\"{ on, attrs }\">\n      \n        <v-btn\n        \n        rounded\n         width=\"width\"\n        height=\"height\"\n        elevation=\"24\"\n          color=\"#E8D5CF\" \n          v-bind=\"attrs\"\n          v-on=\"on\">\n        <v-icon large color=\"green lighten-1\" >mdi-plus</v-icon>\n          <span>TARIFS GRAPHISME</span>\n        </v-btn>\n        \n      </template>\n\n      <v-card class=\"modal_graph\" color=\"#E8D5CF\" >\n     \n        <v-card-title >\n         GRAPHISME\n        </v-card-title>\n        <v-spacer></v-spacer>\n        <v-card-text class=\"prestation\" >\n         Les tarifs indiqués ici sont des tarifs de base et varie selon la complexité du projet, \n         chaque projet étant différend le tarif peut évoluer.\n         \n         <v-card-title>Print</v-card-title>\n         - Carte de visite à partir de 200 euros.<br>\n         - Affiche à partir de 250 euros.<br>\n         - Flyer(recto/verso) à partir de 300 euros.<br>\n         \n         - Logo à partir de 300 euros.<br>\n         <span class=\"condition\"> \n            inclus: planche de propositions de logos(sous minimum 3 axes créatif)</span><br>\n         \n         - Charte graphique à partir de 200 euros.<br>\n         <span class=\"condition\">guide vous permettant l'utilisation de votre logo et de votre charte sur tous vos\n         autres supports de communications (choix typographiques, gammes de couleurs, illustrations,...)\n         </span>\n        </v-card-text>\n\n        <v-divider></v-divider>\n\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn\n            color=\"accent\"\n            text\n            @click=\"dialog = false\"\n          >\n            Merci de votre Attention\n          </v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </div>\n  <br>\n  <!-- ::::::::::::::::::::::::::::::::::::::::::: -->\n  \n  <div class=\"button_illustration\">\n    <v-dialog\n      v-model=\"dialog2\"\n      width=\"900\"\n      height=\"700\"\n    >\n      <template v-slot:activator=\"{ on, attrs }\">\n        <v-btn\n      text--lighten-1\n      width=\"width\"\n        height=\"height\"\n        rounded\n        elevation=\"20\"\n         color=\"#E8D5CF\" \n          v-bind=\"attrs\"\n          v-on=\"on\"\n        ><v-icon large color=\"green lighten-1\" >mdi-plus</v-icon>\n         <span>Tarifs Illustration</span>\n        </v-btn>\n      </template>\n\n      <v-card class=\"modal_illustration\" color=\"#E8D5CF\" >\n     \n        <v-card-title>\n         TARIF\n        </v-card-title>\n        \n        <v-card-text class=\"presta_illustration\">\n        <h5>ILLUSTRATION</h5>\n         Portrait bd 60€ (+5€ par personnes supplémentaire).<br>\n         Faire-part 85€ (mariage, baptème, naissance, anniversaire,...).<br>\n         Décors et mise en scène 20€.<br>\n         Retouche + 10€.<br>\n         Impression sur Demande.<br>\n         Envoie du fichier par Mails au format souhaité.<br>\n         \n        </v-card-text>\n\n        <v-divider></v-divider>\n\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn\n            color=\"accent\"\n            text\n            @click=\"dialog2 = false\"\n          >\n            Merci de votre Attention\n          </v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n    </div>\n  </v-container>\n  </v-img>\n</v-section>\n</template>\n\n\n<!-- ::::::::::::::::::::::::::::::::::::::::::::::::: -->\n\n<style lang=\"scss\" scoped>\n\n\n#pricer{\nbackground-size: cover;\ntop: 0;\nleft: 0;\npadding: 0px 0px 0px;\nmargin: 0px 0px 0px 0px;\nmin-height: 100vh;\nobject-fit: cover;\nbackground-position: center center;\nbackground-attachment: fixed;\nposition: relative;\nz-index: 2;\n}\n\n.pragmatik{\nbackground-size: cover;\nmin-width: 100%;\nmax-height: 100vh;\n}\n\n.pricebox{\n// position: absolute;\ndisplay: flex;\njustify-content: center;\nflex-flow: column wrap;\n    width: fit-content;\n// top: 43%;\n// left: 35%;\nheight: auto;\n    margin-top: 16%;\n// height: 18vh;\n// width:14vw;\n}\n\n.button_graphe.v-btn{\ndisplay: flex;\nwidth: 15vw;\nheight: 13vh;\n}\n\n.button_illustration.v-btn{\ndisplay: flex;\n\nwidth: 15vw;\nheight: 10vh;\n\n}\n\n.modal_graph{\nfont-family: Shadows Into Light Two, sans-serif;\ncolor: black;\nbackground: #E8D5CF;\n}\n.modal_illustration{\nfont-family: Shadows Into Light Two, sans-serif;\ncolor: black;\nbackground-color: #E8D5CF;\n}\n\n\n\n.condition{\nfont-size: 17px;\n}\n.prestation{\nfont-size: 20px;\n}\n\n.presta_illustration\n{\nfont-size: 17px;\n\n}\n</style>\n\n\n\n<script>\n  export default {\n  data() {\n    return {\n      dialog: false,\n      dialog2: false,\n    }\n  },\n\n    computed: {\n      height () {\n        switch (this.$vuetify.breakpoint.height) {\n          case 'xs': return 220\n          case 'sm': return 400\n          case 'md': return 500\n          case 'lg': return 600\n          case 'xl': return 800\n        }\n      },\n        width (){\n    switch (this.$vuetify.breakpoint.width) {\n          case 'xs': return 220\n          case 'sm': return 400\n          case 'md': return 500\n          case 'lg': return 600\n          case 'xl': return 800\n        }   \n      \n      }\n      \n      \n      \n      \n      \n      \n      \n    },\n  }\n</script>\n\n"]}]}